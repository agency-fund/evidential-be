{
  "trusted_ips": [],
  "client_configs": [
    {
      "id": "customer",
      "config": {
        "type": "remote",
        "participants": [
          {
            "participant_type": "groups",
            "table_name": "final_groups",
            "sheet": {
              "url": "${secret:CUSTOMER_CONFIG_SHEET}",
              "worksheet": "DevExplatConfig"
            }
          },
          {
            "participant_type": "alt",
            "table_name": "alt_table",
            "sheet": {
              "url": "${secret:CUSTOMER_CONFIG_SHEET}",
              "worksheet": "DevLinusConfig"
            }
          }
        ],
        "webhook_config": {
          "actions": {
            "commit": {
              "method": "POST",
              "url": "${secret:CUSTOMER_API_BASE}/save-experiment-commit"
            },
            "assignment_file": {
              "method": "GET",
              "url": "${secret:CUSTOMER_API_BASE}/get-file-name-by-experiment-id/{experiment_id}"
            },
            "update_timestamps": {
              "method": "PUT",
              "url": "${secret:CUSTOMER_API_BASE}/update-timestamps-for-experiment"
            },
            "update_description": {
              "method": "PUT",
              "url": "${secret:CUSTOMER_API_BASE}/update-experiment-commit"
            }
          },
          "common_headers": {
            "authorization": "${secret:CUSTOMER_API_AUTH}"
          }
        },
        "dwh": {
          "driver": "postgresql+psycopg2",
          "host": "${secret:CUSTOMER_DWH_HOST}",
          "port": 5439,
          "user": "${secret:CUSTOMER_DWH_USER}",
          "password": "${secret:CUSTOMER_DWH_PASSWORD}",
          "dbname": "customer_dwh_dev",
          "sslmode": "require",
          "search_path": "transforms, linus_transforms"
        }
      }
    }
  ]
}
